<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.a612.springmvc.dao.MessageMapper" >
    <insert id="insert" parameterType="com.a612.springmvc.entity.Message" >
        INSERT INTO message(id,command,description,content) VALUES (#{id},#{command},#{command},#{command})
    </insert>
</mapper>


